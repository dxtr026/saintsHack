{
  "name": "north_face",
  "version": "0.0.1",
  "description": "Bringing Nomads Together",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "lint": "eslint ./src/**/*.js",
    "lint:tests": "eslint ./tests/**/*.spec.js",
    "lintfix": "eslint --fix ./src/**/*.js",
    "compile": "export NODE_ENV=production; sh build.sh",
    "start": "export NODE_PATH=`pwd`/src; node --harmony bin/webpack-dev-server",
    "start:debug": "export NODE_PATH=`pwd`/src; node --harmony --inspect bin/webpack-dev-server",
    "dev": "export NODE_PATH=`pwd`/src; node --harmony bin/webpack-dev-server",
    "prod": "export NODE_ENV=production; export NODE_PATH=`pwd`/server; node server/server/index",
    "prod:debug": "export NODE_ENV=production; export NODE_PATH=`pwd`/src; node --inspect app",
    "compile:worker": "export gulp compile_worker; webpack --config worker-webpack.config.js",
    "dev:nw": "npm run dev -- --nw",
    "dev:no-debug": "npm run dev -- --no_debug",
    "test": "mocha --require tests/config/setup 'tests/**/*.spec.js'",
    "test:cover": "istanbul cover -x *.spec.js _mocha -- -R spec --require tests/config/setup 'tests/**/*.spec.js'",
    "deploy": "npm run lint && npm run compile",
    "test:ava": "ava tests/unit/*.test.js --require babel-register",
    "webPageTest": "node tests/webPageTest/utils/github.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/littlenomadco/north-face.git"
  },
  "authors": [
    "Rahul Yadav <yadav.rahul026@gmail.com>"
  ],
  "license": "MIT",
